# AWS
AWS_REGION=us-east-1
PULUMI_BACKEND_BUCKET=cafe-bot-pulumi-state
PULUMI_SECRETS_ALIAS=alias/cafe-bot-pulumi

# Discord Bot Configuration
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_client_id_here
GUILD_ID=your_guild_id_here
KEYSTORE_CHANNEL_ID=your_keystore_channel_id_here

# Bot Settings
PREFIX=!
OWNER_IDS=owner_id1,owner_id2

# Community Settings
MEMBER_ROLE_ID=your_member_role_id_here
INTRODUCTION_CHANNEL_ID=your_introduction_channel_id_here

# API Keys (optional)
OPENROUTER_API_KEY=

# Logging
LOG_LEVEL=info
LOG_FILE=logs/bot.log

# Development
NODE_ENV=development
